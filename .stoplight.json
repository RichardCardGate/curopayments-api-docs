{
  "exclude": [],
  "formats": {
    "openapi": {
      "rootDir": "reference",
      "include": [
        "**"
      ]
    },
    "json_schema": {
      "rootDir": "models",
      "include": [
        "**"
      ]
    },
    "markdown": {
      "rootDir": "docs",
      "include": [
        "README.md",
        "docs/**"
      ]
    },
    "image": {
      "rootDir": "assets/images"
    }
  }
}